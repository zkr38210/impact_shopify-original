                              assign img_alt = block.settings.stars_icon.alt | escape
                            -%}
                            {%- capture sizes -%}, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2){%- endcapture -%}
                            {{
                              block.settings.stars_icon
                              | image_url: width: 300
                              | image_tag:
                                loading: loading_attribute,
                                width: block.settings.stars_icon.width,
                                height: block.settings.stars_icon.height,
                                sizes: sizes,
                                widths: '450, 535, 750',
                                alt: img_alt
